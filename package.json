{
  "name": "observa",
  "version": "0.2.0",
  "description": "A very simplistic approach to observe on Object.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/observa.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/observa.min.js"
  },
  "keywords": [
    "Observable",
    "Observe",
    "Object",
    "Object.observe",
    "Proxy",
    "observa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/durgesh-priyaranjan/observa"
  },
  "author": "durgesh.priyaranjan@flipkart.com",
  "license": "MIT",
  "dependencies": {
    "object.observe": "0.2.7"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-proxy": "1.1.0",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-polyfill": "6.9.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "clean-webpack-plugin": "^0.1.8",
    "cross-env": "3.1.4",
    "css-loader": "^0.23.1",
    "expose-loader": "0.7.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.9.0",
    "http-server": "0.9.0",
    "imports-loader": "0.6.5",
    "node-libs-browser": "^1.0.0",
    "rollup": "0.39.0",
    "rollup-loader": "0.1.1",
    "rollup-plugin-babel": "2.7.1",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "url-loader": "0.5.7",
    "webpack": "^1.12.13"
  }
}
